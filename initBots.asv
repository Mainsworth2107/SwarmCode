function X = initBots(robots,objs,diffX,diffY)
    global allObjs
    for i = 1:size(robots)
        robots{i} = floor(i/5)
%     robots{i} = robot((rand(3,1).*[diffX;diffY;pi]) - ...
%         (0.5 *[diffX;diffY;0]),i);
    end
    for i = 1:objs
        robots{i}.pose(1:2) = allObjs(i,(1:2));
    end
    X = robots;
end